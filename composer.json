{
    "name": "chi-teck/zoho-crm-connector",
    "description": "Zoho CRM connector",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.1.0",
        "guzzlehttp/guzzle": "^7.5",
        "psr/http-client": "^1.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/console": "^6.1",
        "symfony/var-dumper": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "ZohoCrmConnector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZohoCrmConnector\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
